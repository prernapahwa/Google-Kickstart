#include<bits/stdc++.h>
using namespace std;
int main() {
    int t;
    cin>>t;
    int T=0;
    while(++T<=t) {
        int n;
        cin>>n;
        string s;
        cin>>s;
        cout<<"Case #"<<T<<": ";
        int ans=1;
        cout<<ans<<" ";
        for(int i=1;i<n;i++) {
            if(s[i]>s[i-1]) {
                ans+=1;
            }
            else {
                ans=1;
            }
            cout<<ans<<" ";
        }
        cout<<endl;
    }
}
